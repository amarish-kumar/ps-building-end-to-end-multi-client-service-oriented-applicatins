<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <connectionStrings>
      <add name="CarRental" connectionString="data source=localhost\sqlexpress;initial catalog=CarRental;Trusted_Connection=True;" providerName="System.Data.SqlClient" />
    </connectionStrings>
  
    <system.serviceModel>
      <services>
        <service name="CarRental.Business.Managers.InventoryManager">
          <endpoint 
           address="net.tcp://localhost:8009/InventoryService"
           binding="netTcpBinding"
           contract="CarRental.Business.Contracts.IInventoryService"
          />
        </service>
      </services>
    </system.serviceModel>
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
</configuration>